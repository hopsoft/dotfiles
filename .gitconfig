[color]
	ui = true
[user]
	name = Nathan Hopkins
	email = natehop@gmail.com
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[branch "master"]
	remote = origin
	merge = refs/heads/master
[core]
	excludesfile = /Users/nhopkins/.gitignore
[difftool "opendiff"]
	cmd = /usr/bin/opendiff $LOCAL $REMOTE
[mergetool "opendiff"]
  cmd = /usr/bin/opendiff $LOCAL $REMOTE -ancestor $BASE -merge $MERGED
	trustExitCode = true
[push]
	default = simple
