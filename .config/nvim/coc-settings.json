{
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": ["vim"],
  "git.addGBlameToVirtualText": false,
  "git.blameUseRealTime": true,
  "git.enableGlobalStatus": true,
  "git.enableGutters": true,
  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
  "prettier.enable": true,
  "prettier.tabWidth": 2,
  "prettier.useEditorConfig": true,
  "prettier.bracketSameLine": true,
  "prettier.singleQuote": true,
  "prettier.printWidth": 110,
  "prettier.arrowParens": "avoid",
  "prettier.trailingComma": "none",
  "prettier.bracketSpacing": false,
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.endOfLine": "lf",
  "prettier.semi": false,
  "solargraph.logLevel": "info",
  "solargraph.checkGemVersion": true,
  "markdownlint.onChange": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.headwind.removeDuplicates": true,
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "solargraph.diagnostics": true,
  "solargraph.folding": false,
  "solargraph.formatting": true,
  "solargraph.promptDownload": true,
  "solargraph.references": true,
  "solargraph.rename": true,
  "solargraph.symbols": true,
  "docker.enable": true,
  "htmlhint.enable": true,
  "htmlhint.options": {
    "tagname-lowercase": true,
    "attr-lowercase": true,
    "attr-value-double-quotes": true,
    "attr-value-not-empty": false,
    "attr-no-duplication": true,
    "doctype-first": true,
    "tag-pair": true,
    "tag-self-close": false,
    "spec-char-escape": true,
    "id-unique": true,
    "src-not-empty": true,
    "title-require": true
  },
  "html.filetypes": [
    "erb",
    "html",
    "handlebars",
    "htmldjango",
    "blade"
  ],
  "yaml.enable": true,
  "yaml.format.printWidth": 110,
  "svg.completion.insertCloseTagSign": true,
  "svg.format.plugins": {
    "sortAttrs": true
  },
  "css.validate": true,
  "solargraph.useBundler": false
}
